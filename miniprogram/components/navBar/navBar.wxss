@import "../../assets/fonts/iconfont.wxss";

.navbar{
  position: fixed;
  top: 0;
  width: 100%;
  height: 128rpx;
  line-height: 162rpx;
  background: white;
  text-align: center;
  font-weight: bold;
  font-size: 38rpx;
  z-index: 999;
}

.nav-back{
  position: fixed;
  top: 54rpx;
  left: 12rpx;
  height: 52rpx;
  width: 52rpx;
  z-index:9999;
  overflow: hidden;
}

.nav-back-mask{
  position: absolute;
  background: black;
  height: 52rpx;
  width: 52rpx;
  border-radius: 50%;
  opacity: 0.3;
}

.navbar-icon{
  position: fixed;
  top: 54rpx;
  left: 12rpx;
  color: black;
  height: 52rpx;
  line-height: 52rpx;
  width: 52rpx;
  text-align: center;
  font-size: 38rpx;
  font-weight: bold;
}
